{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": "Business services and offerings"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "features": {
      "type": "json"
    },
    "icon": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "priceUnit": {
      "type": "string",
      "default": "per project"
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
